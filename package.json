{
  "name": "leetvision",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered browser extension for code assistance and learning",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "setup": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristong123/LeetVision.git"
  },
  "keywords": [
    "leetcode",
    "neetcode",
    "ai",
    "code-assistant",
    "browser-extension",
    "learning"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristong123/LeetVision/issues"
  },
  "homepage": "https://github.com/kristong123/LeetVision#readme",
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "amazon-cognito-identity-js": "^6.3.16",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "webextension-polyfill": "^0.12.0"
  }
}
